    <item android:bottom="@dimen/_4sdp">
        <shape>
            <corners android:radius="@dimen/_40sdp" />
            <gradient
                android:angle="315"

                android:endColor="#7EE0D9"
                android:startColor="#A8C2FC"
                android:type="linear"

                />
        </shape>
    </item>
</layer-list>