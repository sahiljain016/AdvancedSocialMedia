<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_1"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_3_1"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/end3"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_2_1"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/end2"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_1_2"
        motion:constraintSetEnd="@+id/end2"
        motion:constraintSetStart="@id/end"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_1_3"
        motion:constraintSetEnd="@+id/end3"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:constraintSetStart="@id/end"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_2_3"
        motion:constraintSetEnd="@+id/end3"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:constraintSetStart="@id/end2"
        motion:duration="1000">
    </Transition>

    <Transition
        android:id="@+id/TRANS_BI_FF_EXP_3_2"
        motion:constraintSetEnd="@+id/end2"
        motion:motionInterpolator="@string/material_motion_easing_decelerated"
        motion:constraintSetStart="@id/end3"
        motion:duration="1000">
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/V_INDICATOR_LONG_FF_EXP"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            motion:layout_constraintBottom_toBottomOf="@id/V_DOT_1_BOTTOM_FF_EXP"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_1_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/V_INDICATOR_LONG_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_2_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/V_INDICATOR_LONG_FF_EXP"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            motion:layout_constraintBottom_toBottomOf="@id/V_DOT_1_BOTTOM_FF_EXP"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_1_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/V_INDICATOR_LONG_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_2_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end2">
        <Constraint
            android:id="@+id/V_INDICATOR_LONG_FF_EXP"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/V_DOT_1_BOTTOM_FF_EXP"
            motion:layout_constraintTop_toTopOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_1_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/V_INDICATOR_LONG_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_2_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/V_INDICATOR_LONG_FF_EXP"/>

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end3">
        <Constraint
            android:id="@+id/V_INDICATOR_LONG_FF_EXP"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="@id/V_DOT_1_BOTTOM_FF_EXP"
            motion:layout_constraintTop_toTopOf="@id/V_DOT_1_BOTTOM_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_1_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/V_DOT_2_BOTTOM_FF_EXP"/>

        <Constraint
            android:id="@+id/V_DOT_2_BOTTOM_FF_EXP"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/V_INDICATOR_LONG_FF_EXP"/>

    </ConstraintSet>
</MotionScene>